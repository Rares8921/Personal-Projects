<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox fx:id="root" style="-fx-background-color: #312e2b;" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/16" fx:controller="sample.application.endedbyuser.EndedByUserController">
	<HBox alignment="CENTER">
		<VBox.margin>
			<Insets bottom="10.0" left="25.0" right="25.0" top="30.0" />
		</VBox.margin>
		<Text fx:id="message" fill="WHITE" styleClass="message">
         <font>
            <Font name="Verdana" size="12.0" />
         </font></Text>
	</HBox>
	<Region VBox.vgrow="ALWAYS" />
	<BorderPane>
		<VBox.margin>
			<Insets bottom="10.0" left="25.0" right="25.0" top="0.0" />
		</VBox.margin>
		<left>
			<Button fx:id="cancelButton" onAction="#cancelButtonAction" styleClass="actionButton" text="Cancel" textFill="WHITE">
            <font>
               <Font name="Verdana" size="12.0" />
            </font></Button>
		</left>
		<right>
			<Button fx:id="acceptButton" onAction="#acceptButtonAction" styleClass="actionButton" text="Accept" textFill="WHITE">
            <font>
               <Font name="Verdana" size="12.0" />
            </font></Button>
		</right>
	</BorderPane>
	<stylesheets>
		<URL value="@EndedByUser.css" />
	</stylesheets>
</VBox>
